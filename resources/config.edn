                          :post-transplant-baseline-cifs {:centre {:column :A
                                                                   :match "centre"}
                                                          :days {:column :B
                                                                 :match "(time)|(days)"}
                                                          :cif-post-transplant {:column :C
                                                                                :match "graft"}}

                          :post-transplant-baseline-vars {:factor {:column :A
                                                                   :match "factor"}
