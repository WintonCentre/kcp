                                                       :cif-from-listing {:column :C
                                                                          :match "survival"}}
                          :from-listing-baseline-vars {:factor {:column :A
                                                                :match "factor"}
                                                       :level {:column :B
                                                               :match "level"}}
                          :from-listing-inputs {:factor {:column :A
                                                         :match "factor"}
                                                :level {:column :B
                                                        :match "level"}
                                                :factor-name {:column :C
                                                              :match "inputs"}
                                                :level-name {:column :D
                                                             :match "(label)|(level)"}
                                                :beta-from-listing {:column :E
                                                                    :match "survival"}
                                                :type {:column :F
                                                       :match "type"}
                                                :sub-text {:column :G
